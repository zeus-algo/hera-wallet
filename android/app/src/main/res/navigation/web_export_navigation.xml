<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/webExportNavigation"
    app:startDestination="@id/webExportAccountSelectionFragment">

    <action
        android:id="@+id/action_webExportNavigation_pop"
        app:popUpTo="@id/accountsQrScannerFragment"
        app:popUpToInclusive="true" />

    <fragment
        android:id="@+id/webExportAccountSelectionFragment"
        android:name="network.voi.hera.modules.webexport.accountselection.ui.WebExportAccountSelectionFragment"
        android:label="fragment_webexport_account_selection">
        <argument
            android:name="backupId"
            app:argType="string" />
        <argument
            android:name="modificationKey"
            app:argType="string" />
        <argument
            android:name="encryptionKey"
            app:argType="string" />
        <action
            android:id="@+id/action_webExportAccountSelectionFragment_to_webExportDomainNameConfirmationFragment"
            app:destination="@id/webExportDomainNameConfirmationFragment">
            <argument
                android:name="backupId"
                app:argType="string" />
            <argument
                android:name="modificationKey"
                app:argType="string" />
            <argument
                android:name="encryptionKey"
                app:argType="string" />
            <argument
                android:name="accountList"
                app:argType="string[]"/>
        </action>
    </fragment>

    <fragment
        android:id="@+id/webExportDomainNameConfirmationFragment"
        android:name="network.voi.hera.modules.webexport.domainnameconfirmation.ui.WebExportDomainNameConfirmationFragment"
        android:label="fragment_webexport_domainname_confirmation">
        <argument
            android:name="backupId"
            app:argType="string" />
        <argument
            android:name="modificationKey"
            app:argType="string" />
        <argument
            android:name="encryptionKey"
            app:argType="string" />
        <argument
            android:name="accountList"
            app:argType="string[]" />
        <action
            android:id="@+id/action_webExportDomainNameConfirmationFragment_to_webExportAccountConfirmationFragment"
            app:destination="@id/webExportAccountConfirmationFragment">
            <argument
                android:name="backupId"
                app:argType="string" />
            <argument
                android:name="modificationKey"
                app:argType="string" />
            <argument
                android:name="encryptionKey"
                app:argType="string" />
            <argument
                android:name="accountList"
                app:argType="string[]" />
        </action>
        <action
            android:id="@+id/action_webExportDomainNameConfirmationFragment_to_webExportPasswordFragment"
            app:destination="@id/webExportPasswordFragment">
            <argument
                android:name="backupId"
                app:argType="string"
                app:nullable="false" />
            <argument
                android:name="modificationKey"
                app:argType="string"
                app:nullable="false" />
            <argument
                android:name="encryptionKey"
                app:argType="string"
                app:nullable="false" />
            <argument
                android:name="accountList"
                app:argType="string[]" />
        </action>
    </fragment>

    <fragment
        android:id="@+id/webExportPasswordFragment"
        android:name="network.voi.hera.modules.webexport.pinentry.ui.WebExportPasswordFragment"
        android:label="fragment_webexport_password">
        <argument
            android:name="backupId"
            app:argType="string"
            app:nullable="false" />
        <argument
            android:name="modificationKey"
            app:argType="string"
            app:nullable="false" />
        <argument
            android:name="encryptionKey"
            app:argType="string"
            app:nullable="false" />
        <argument
            android:name="accountList"
            app:argType="string[]" />
        <action
            android:id="@+id/action_webExportPasswordFragment_to_webExportAccountConfirmationFragment"
            app:destination="@id/webExportAccountConfirmationFragment"
            app:popUpTo="@id/webExportDomainNameConfirmationFragment">
            <argument
                android:name="backupId"
                app:argType="string"
                app:nullable="false" />
            <argument
                android:name="modificationKey"
                app:argType="string"
                app:nullable="false" />
            <argument
                android:name="encryptionKey"
                app:argType="string"
                app:nullable="false" />
            <argument
                android:name="accountList"
                app:argType="string[]" />
        </action>
    </fragment>

    <fragment
        android:id="@+id/webExportAccountConfirmationFragment"
        android:name="network.voi.hera.modules.webexport.accountconfirmation.ui.WebExportAccountConfirmationFragment"
        android:label="fragment_webexport_account_confirmation">
        <argument
            android:name="backupId"
            app:argType="string" />
        <argument
            android:name="modificationKey"
            app:argType="string" />
        <argument
            android:name="encryptionKey"
            app:argType="string" />
        <argument
            android:name="accountList"
            app:argType="string[]" />
        <action
            android:id="@+id/action_webExportAccountConfirmationFragment_to_webExportSuccessResultFragment"
            app:destination="@id/webExportSuccessResultFragment" />
    </fragment>

    <fragment
        android:id="@+id/webExportSuccessResultFragment"
        android:name="network.voi.hera.modules.webexport.exportresult.ui.WebExportSuccessResultFragment"
        android:label="WebExportSuccessResultFragment" />

</navigation>
